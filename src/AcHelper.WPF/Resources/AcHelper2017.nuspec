<?xml version="1.0" encoding="utf-8" ?>
<package >

  <metadata>
    <id>AcHelper.WPF.2017</id>
    <version>$version$</version>
    <title>AcHelper WPF</title>
    <authors>Sander van Bueren (BuerTech)</authors>
    <owners>BuerTech</owners>
    <description>Contains functionalities for WPF in combination with AutoCAD.</description>

    <projectUrl>https://github.com/BikerMouse/AcHelper</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <releaseNotes>First release as a version separated assembly.</releaseNotes>
    <copyright>Copyright © BuerTech 2018</copyright>
    <tags>AutoCAD BuerTech</tags>

    <dependencies>
      <dependency id="MvvmLightLibs" version="5.3.0" />
      <dependency id="AutoCAD.Net" version="21.0.2" exclude="runtime" />
      <dependency id="AutoCAD.Net.Core" version="21.0.2" exclude="runtime" />
      <dependency id="AutoCAD.Net.Model" version="21.0.2" exclude="runtime" />
    </dependencies>
  </metadata>

  <files>
    <file src="Release\2017\**" target="lib\net46"/>
    <file src="Release\2017\**" target="build\x86"/>

    <file src="x64\Release\2017\**" target="build\x64"/>

    <file src="AcHelper.WPF.props" target="build\net46"/>
  </files>
</package>